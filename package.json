{
  "name": "@panhezeng/tinymce-plugin-text-indent-outdent",
  "version": "0.0.0-development",
  "description": "A tinymce plugin text indent outdent",
  "keywords": [
    "tinymce plugin",
    "text indent"
  ],
  "homepage": "https://github.com/panhezeng/tinymce-plugin-text-indent-outdent#readme",
  "bugs": {
    "url": "https://github.com/panhezeng/tinymce-plugin-text-indent-outdent/issues",
    "email": "panhezeng@gmail.com"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "潘何增",
    "email": "panhezeng@gmail.com",
    "url": "http://apsay.com/"
  },
  "contributors": [],
  "files": [
    "dist",
    "src",
    "package.json",
    "README"
  ],
  "main": "",
  "browser": "dist/textindentoutdent/plugin.min.js",
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/panhezeng/tinymce-plugin-text-indent-outdent.git"
  },
  "scripts": {
    "update": "npm update",
    "lint": "eslint --fix 'src/**/*.{js,ts,jsx,vue}'",
    "build": "npm run lint && webpack --mode production",
    "example:install": "cd example && npm install",
    "example:update": "cd example && npm update",
    "example:dev": "cd example && npm run dev",
    "example:build": "cd example && npm run build",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@semantic-release/changelog": "latest",
    "commitizen": "latest",
    "coveralls": "latest",
    "cz-conventional-changelog": "latest",
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "clean-webpack-plugin": "latest",
    "copy-webpack-plugin": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "tinymce": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "semantic-release": "latest"
  },
  "peerDependencies": {
    "tinymce": "latest"
  },
  "engines": {
    "node": ">=13",
    "npm": ">=6"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,vue,html,css,less,scss,sass,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "maintained node versions",
    "not dead",
    "> 1%",
    "Firefox ESR",
    "ie 11"
  ],
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog"
    ]
  }
}
