{
  "name": "@panhezeng/tinymce-plugin-text-indent-outdent",
  "version": "0.0.0-development",
  "description": "A tinymce plugin text indent outdent",
  "keywords": [
    "tinymce plugin",
    "text indent"
  ],
  "homepage": "https://github.com/panhezeng/tinymce-plugin-text-indent-outdent#readme",
  "bugs": {
    "url": "https://github.com/panhezeng/tinymce-plugin-text-indent-outdent/issues",
    "email": "panhezeng@gmail.com"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "潘何增",
    "email": "panhezeng@gmail.com",
    "url": "http://apsay.com/"
  },
  "contributors": [],
  "files": [
    "dist",
    "src",
    "package.json",
    "README"
  ],
  "main": "",
  "browser": "dist/tinymce-plugin-text-indent-outdent/plugin.min.js",
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/panhezeng/tinymce-plugin-text-indent-outdent.git"
  },
  "scripts": {
    "update": "npm update",
    "lint": "eslint --fix 'src/**/*.{js,ts,jsx,vue}'",
    "build": "npm run lint && webpack --mode production",
    "example:install": "cd example && npm install",
    "example:update": "cd example && npm update",
    "example:dev": "cd example && npm run dev",
    "example:build": "cd example && npm run build",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@semantic-release/changelog": "^3.0.6",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.9",
    "cz-conventional-changelog": "^3.0.2",
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "tinymce": "~5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "semantic-release": "^15.13.31"
  },
  "peerDependencies": {
    "tinymce": "~5"
  },
  "engines": {
    "node": ">=13",
    "npm": ">=6"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,vue,html,css,less,scss,sass,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "maintained node versions",
    "not dead",
    "> 1%",
    "Firefox ESR",
    "ie 11"
  ],
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog"
    ]
  }
}
